            "charAnim1": "smile1",
            "charAnim2": "blank",
            "charLabel": "hiori",
            "charLipAnim": "lip_smile",
            "id": 1002001050130,
            "speaker": "히오리",
            "text": "――集まってくれた皆さん",
            "text-ko": "- 모여 주신 여러분",
	"text-patch": "――여기 와주신 여려분",
            "textCtrl": "p",
            "textFrame": "001",
            "voice": "produce_events/100200105/1002001050130"
        },
        {
            "charLabel": "hiori",
            "charLipAnim": "lip_smile",
            "id": 1002001050140,
            "speaker": "히오리",
            "text": "本当に……ありがとうございました！",
            "text-ko": "정말 ...... 감사합니다!",
	"text-patch": "정말..... 고마워요!!",
            "textCtrl": "p",
            "textFrame": "001",
            "voice": "produce_events/100200105/1002001050140"
        },
